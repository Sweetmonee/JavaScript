name = 'Mike';
age = 30;
console.log(age);
#덮어씌워질 가능성 있어서 위험함
let name = 'Mike';
let age = '30';
#이렇게하면 선언되어있는지 확인가능
#const = 절대로 바뀌지 않는 상수
#파이 속도 생일과 같은 값을 입력할때 사용
일단 const로 선언하고 바뀔거 같을때 let으로 다시 선언
const message4 = '나는 ${30+1}살 입니다.';
console.log(message4)
const name = 'Mike';
console.log(typeof massage4);
